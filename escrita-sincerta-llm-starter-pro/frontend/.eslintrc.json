{
  "extends": "@vue/cli-plugin-eslint/recommended",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": ["react"],
  "env": {
    "node": true,
    "browser": true,
    "es6": true
  },
  "parser": "@typescript-eslint/parser",
  "rules": {
    "semioptimized.vars-used-only-once": ["error"],
    "no-console": "off",
    "react/prop-types": "off",
    "import/prefer-default-export": "off",
    "@vue/cli-plugin-eslint/prefer-arrow-callback": "off",
    "max-len/deny-large-function-expressions": ["error"],
    "no-console": "1"
  }
}